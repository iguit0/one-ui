  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prebuild": "cd src",
    "copy-css": "cpy --cwd=\"src\" \"**/*.{scss,svg}\" \"../dist\" --parents",
    "build": "tsc && npm run copy-css",
    "postbuild": "cd ..",
